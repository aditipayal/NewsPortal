<script type="text/javascript">
    function deferredItems() {

        var deferredScriptElem = document.getElementById('deferred_javascript');
        var scriptBody = deferredScriptElem.innerHTML;
        var jsCode = scriptBody.replace(/(<!--|-->|<script>|<\/script>)/g, "");
        (0, eval)(jsCode);
    }
    function deferredLoad() {
        var scrollTop = '';
        var $renderTaboola = $('#render-taboola');
        var taboolaHeight = $renderTaboola.length ? $renderTaboola.data('height') : 300;
        $(window).bind('scroll.functionLoadScriptOnScroll', function () {
            scrollTop = $(this).scrollTop();            
            if (scrollTop >= taboolaHeight) {
                typeof loadScriptOnScroll === 'function' ? loadScriptOnScroll() : 0;
                $(window).unbind('.functionLoadScriptOnScroll');
            }
        });
    }
    window.addEventListener ? window.addEventListener('DOMContentLoaded', deferredItems, false) : window.onload = deferredItems;
    window.addEventListener ? window.addEventListener('load', deferredLoad, false) : window.onload = deferredLoad;
</script>
    

